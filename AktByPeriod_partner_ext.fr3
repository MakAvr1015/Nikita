    <TfrxFIBQuery Name="dsResult" UserName="dsResult" CloseDataSource="True" FieldAliases.Text="" BCDToCurrency="False" IgnoreDupParams="False" SQL.Text="select * from&#13;&#10;(&#13;&#10;select&#13;&#10;  (select f_name from SP_T_NSI_DOC_OUT_TYPES_GET(f_type)) as Doc_name,&#13;&#10;  f_date,&#13;&#10;  f_number as f_out_number,&#13;&#10;  f_doc_sum,&#13;&#10;  null as f_pay_number,&#13;&#10;  null as f_pay_sum&#13;&#10;from&#13;&#10;  t_doc_out d&#13;&#10;&#13;&#10;  where f_date between :f_start and :f_end and f_partner=:f_partner&#13;&#10;  and f_state&#62;=3&#13;&#10;  and coalesce(F_MONEY_TYPE,1)=:F_MONEY_TYPE&#13;&#10; union all&#13;&#10;select&#13;&#10;  (select f_name from SP_T_NSI_DOC_IN_TYPES_GET(f_type)),&#13;&#10;  f_date,&#13;&#10;  null,&#13;&#10;  null,&#13;&#10;  f_number,&#13;&#10;  f_doc_sum&#13;&#10;&#13;&#10;from&#13;&#10;  t_doc_in where f_date between :f_start and :f_end and f_partner=:f_partner&#13;&#10;  and f_state&#62;=3&#13;&#10;  and :F_MONEY_TYPE=1&#13;&#10;union all&#13;&#10;select&#13;&#10;  'Входящий платеж',&#13;&#10;  f_date,&#13;&#10;  null,&#13;&#10;  null,&#13;&#10;  f_number,&#13;&#10;  f_summa&#13;&#10;from&#13;&#10;  t_money_in where f_date between :f_start and :f_end and f_partner=:f_partner&#13;&#10;  and f_state&#62;=3&#13;&#10;  and :F_MONEY_TYPE=iif(F_TYPE=2,2,1)&#13;&#10;union all&#13;&#10;select&#13;&#10;  'Исходящий платеж',&#13;&#10;  f_date,&#13;&#10;  f_number,&#13;&#10;  f_summa,&#13;&#10;  null,&#13;&#10;  null&#13;&#10;&#13;&#10;from&#13;&#10;  t_money_out where f_date between :f_start and :f_end and f_partner=:f_partner&#13;&#10;  and f_state&#62;=3&#13;&#10;&#13;&#10;)&#13;&#10;order by f_date" PropData=""/>
    <TfrxFIBQuery Name="dsStartBalans" UserName="dsStartBalans" CloseDataSource="True" FieldAliases.Text="" BCDToCurrency="False" IgnoreDupParams="False" SQL.Text="select&#13;&#10;coalesce((select sum(F_DOC_SUM)&#13;&#10;                 from T_DOC_OUT&#13;&#10;                 where F_PARTNER = :F_PARTNER and&#13;&#10;                       F_DATE &#60; iif(cast('10.01.2012' as date) &#62; :F_START, :F_START, cast('10.01.2012' as date)) and&#13;&#10;                       F_STATE &#62;= 3 and&#13;&#10;                       :F_MONEY_TYPE = 1), 0 )+&#13;&#10;       coalesce((select sum(F_DOC_SUM)&#13;&#10;                   from T_DOC_OUT&#13;&#10;                   where F_PARTNER = :F_PARTNER and&#13;&#10;                      F_DATE between cast('10.01.2012' as date) and :F_START and&#13;&#10;                                                                F_STATE &#62;= 3 and&#13;&#10;                                                                iif(F_MONEY_TYPE = 2, 2, 1) = :f_money_type), 0 )&#13;&#10;&#13;&#10;&#13;&#10; -&#13;&#10;  coalesce((select sum(f_doc_sum) from t_doc_in where f_partner=:f_partner&#13;&#10;    and f_date&#60;:f_start and f_state&#62;=3 and :f_money_type=1),0)&#13;&#10;-(&#13;&#10;&#13;&#10;  coalesce(&#13;&#10;  (select sum(f_summa) from t_money_in where f_partner=:f_partner&#13;&#10;    and F_DATE &#60; iif(cast('10.01.2012' as date) &#62; :F_START, :f_start, cast('10.01.2012' as date)) and&#13;&#10;                       F_STATE &#62;= 3 and&#13;&#10;                       :F_MONEY_TYPE = 1),0) +&#13;&#10;  coalesce((select sum(f_summa) from t_money_in where f_partner=:f_partner&#13;&#10;    and F_DATE between cast('10.01.2012' as date) and :F_START and&#13;&#10;                                                                F_STATE &#62;= 3 and&#13;&#10;                                                                iif(F_TYPE = 2, 2, 1) = :f_money_type),0)&#13;&#10;  )&#13;&#10;  as f_balans&#13;&#10;from RDB$DATABASE" PropData="